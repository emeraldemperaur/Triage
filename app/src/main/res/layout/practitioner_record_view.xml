<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/silver"
    android:id="@+id/practitioner_detail_view"
    android:layout_height="match_parent">

    <androidx.core.widget.NestedScrollView
        android:id="@+id/practitioner_info_scroll_view"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="-365dp">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:visibility="visible">


            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/practitioner_detail_window"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/chiron_blue"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toTopOf="parent">


                <TextView
                    android:id="@+id/practitioner_detail_first_name_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="6dp"
                    android:layout_marginTop="69dp"
                    android:fontFamily="@font/montserratlight"
                    android:letterSpacing="0.21"
                    android:text="@string/first_name"
                    android:textAllCaps="true"
                    android:textColor="@color/chiron_grey"
                    android:textSize="17sp"
                    android:textStyle="bold"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <TextView
                    android:id="@+id/practitioner_detail_first_name_text"
                    android:layout_width="330dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="6dp"
                    android:layout_marginTop="1dp"
                    android:layout_marginEnd="6dp"
                    android:ellipsize="end"
                    android:fontFamily="@font/montserratlight"
                    android:letterSpacing="0.06"
                    android:maxLines="2"
                    android:text="@string/thelonious"
                    android:textColor="@color/white"
                    android:textSize="26sp"
                    android:textStyle="bold"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/practitioner_detail_first_name_title" />


                <TextView
                    android:id="@+id/practitioner_detail_last_name_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="6dp"
                    android:layout_marginTop="12dp"
                    android:fontFamily="@font/montserratlight"
                    android:letterSpacing="0.21"
                    android:text="@string/last_name"
                    android:textAllCaps="true"
                    android:textColor="@color/chiron_grey"
                    android:textSize="17sp"
                    android:textStyle="bold"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/practitioner_detail_first_name_text" />

                <TextView
                    android:id="@+id/practitioner_detail_last_name_text"
                    android:layout_width="300dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="6dp"
                    android:layout_marginTop="3dp"
                    android:fontFamily="@font/montserratlight"
                    android:letterSpacing="0.06"
                    android:text="@string/jaha"
                    android:textAllCaps="false"
                    android:maxLines="2"
                    android:ellipsize="end"
                    android:textColor="@color/white"
                    android:textSize="26sp"
                    android:textStyle="bold"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/practitioner_detail_last_name_title" />

                <TextView
                    android:visibility="visible"
                    android:id="@+id/practitioner_detail_speciality"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="6dp"
                    android:layout_marginTop="3dp"
                    android:fontFamily="@font/montserratthin"
                    android:letterSpacing="0.06"
                    android:text="@string/general_practitioner"
                    android:textAllCaps="true"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:textColor="@color/white"
                    android:maxLength="20"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    app:layout_constraintLeft_toLeftOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/practitioner_detail_last_name_text" />


                <TextView
                    android:id="@+id/practitioner_detail_practitioner_id_title"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="0dp"
                    android:layout_marginEnd="13dp"
                    android:layout_marginTop="33dp"
                    android:fontFamily="@font/montserratlight"
                    android:letterSpacing="0.21"
                    android:text="@string/practitioner_id"
                    android:textAllCaps="true"
                    android:textColor="@color/chiron_grey"
                    android:textSize="17sp"
                    android:textStyle="bold"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/practitioner_detail_last_name_text" />


                <TextView
                    android:id="@+id/practitioner_detail_practitioner_id_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="6dp"
                    android:layout_marginEnd="13dp"
                    android:layout_marginBottom="12dp"
                    android:layout_marginTop="1dp"
                    android:fontFamily="@font/montserratthin"
                    android:letterSpacing="0.06"
                    android:maxLines="2"
                    android:ellipsize="end"
                    android:text="@string/practitioner_id"
                    android:textColor="@color/white"
                    android:textSize="26sp"
                    android:textStyle="bold"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/practitioner_detail_practitioner_id_title" />


                <ImageView
                    android:id="@+id/practitioner_detail_profile"
                    android:layout_width="69dp"
                    android:layout_height="69dp"
                    android:layout_marginTop="144dp"
                    android:layout_marginEnd="23dp"
                    android:contentDescription="@string/profile_icon"
                    android:scaleType="fitXY"
                    android:src="@drawable/profile_outline"
                    app:layout_constraintRight_toRightOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:tint="@color/chiron_grey" />




            </androidx.constraintlayout.widget.ConstraintLayout>



            <LinearLayout
                android:visibility="visible"
                android:id="@+id/practitioner_detail_info_view"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:background="@drawable/silver"
                app:layout_constraintLeft_toLeftOf="parent"
                app:layout_constraintRight_toRightOf="parent"
                app:layout_constraintTop_toBottomOf="@id/practitioner_detail_window">

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="0dp"
                    android:layout_marginBottom="9dp"
                    android:background="@color/chiron_grey"
                    android:elevation="13dp"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:visibility="visible" />

                <TextView
                    android:id="@+id/practitioner_detail_contact_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="6dp"
                    android:fontFamily="@font/montserratlight"
                    android:letterSpacing="0.21"
                    android:text="@string/contact"
                    android:textAllCaps="true"
                    android:textColor="@color/chiron_blue"
                    android:textSize="19sp"
                    android:textStyle="bold" />

                <TextView
                    android:id="@+id/practitioner_detail_contact_info_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginTop="3dp"
                    android:fontFamily="@font/montserratbold"
                    android:letterSpacing="0.21"
                    android:text="@string/information"
                    android:textAllCaps="true"
                    android:textColor="@color/chiron_blue"
                    android:textSize="13sp"
                    android:textStyle="bold" />



                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_marginTop="6dp"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                    <TextView
                        android:id="@+id/practitioner_detail_contact_phone_title"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        android:layout_marginTop="13dp"
                        android:layout_marginStart="6dp"
                        android:layout_marginEnd="12dp"
                        android:layout_marginBottom="12dp"
                        android:textAlignment="center"
                        android:textStyle="bold"
                        android:letterSpacing="0.06"
                        android:textColor="@color/chiron_blue"
                        android:textAllCaps="true"
                        android:fontFamily="@font/montserratsemibold"
                        android:text="@string/phone"
                        android:textSize="16sp"
                        android:maxLines="21"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>


                    <TextView
                        android:id="@+id/practitioner_detail_contact_phone_text"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/practitioner_detail_contact_phone_title"
                        android:layout_marginTop="0dp"
                        android:layout_marginStart="6dp"
                        android:layout_marginEnd="12dp"
                        android:layout_marginBottom="12dp"
                        android:textAlignment="center"
                        android:textStyle="normal"
                        android:letterSpacing="0.21"
                        android:textColor="@color/chiron_blue"
                        android:textAllCaps="true"
                        android:fontFamily="@font/montserratlight"
                        android:text="@string/_1_212_113_4564"
                        android:textSize="18sp"
                        android:maxLines="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>


                    <TextView
                        android:id="@+id/practitioner_detail_contact_email_title"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/practitioner_detail_contact_phone_text"
                        android:layout_marginTop="13dp"
                        android:layout_marginStart="6dp"
                        android:layout_marginEnd="12dp"
                        android:layout_marginBottom="12dp"
                        android:textAlignment="center"
                        android:textStyle="bold"
                        android:letterSpacing="0.06"
                        android:textColor="@color/chiron_blue"
                        android:textAllCaps="true"
                        android:fontFamily="@font/montserratsemibold"
                        android:text="@string/email"
                        android:textSize="16sp"
                        android:maxLines="21"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>


                    <TextView
                        android:id="@+id/practitioner_detail_contact_email_text"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintTop_toBottomOf="@id/practitioner_detail_contact_email_title"
                        android:layout_marginTop="0dp"
                        android:layout_marginStart="6dp"
                        android:layout_marginEnd="12dp"
                        android:layout_marginBottom="12dp"
                        android:textAlignment="center"
                        android:textStyle="normal"
                        android:letterSpacing="0.21"
                        android:textColor="@color/chiron_blue"
                        android:textAllCaps="false"
                        android:fontFamily="@font/montserratlight"
                        android:text="@string/test_test_com"
                        android:textSize="18sp"
                        android:maxLines="1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"/>

                    <View
                        android:id="@+id/practitioner_detail_bottom_line"
                        app:layout_constraintTop_toBottomOf="@id/practitioner_detail_contact_email_text"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="33dp"
                        android:layout_marginBottom="9dp"
                        android:background="@color/chiron_grey"
                        android:elevation="13dp"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:visibility="visible" />


                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/practitioner_detail_contact_phone_button"
                        android:enabled="false"
                        app:layout_constraintTop_toBottomOf="@id/practitioner_detail_bottom_line"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        android:layout_marginTop="33dp"
                        app:icon="@drawable/ic_baseline_local_phone_24"
                        app:iconGravity="textStart"
                        android:text="@string/call"
                        android:textSize="17sp"
                        android:paddingTop="23dp"
                        android:paddingBottom="23dp"
                        android:fontFamily="@font/montserratbold"
                        android:layout_width="320dp"
                        android:layout_height="wrap_content"/>

                    <com.google.android.material.button.MaterialButton
                        android:id="@+id/practitioner_detail_contact_email_button"
                        app:layout_constraintTop_toBottomOf="@+id/practitioner_detail_contact_phone_button"
                        app:layout_constraintLeft_toLeftOf="parent"
                        app:layout_constraintRight_toRightOf="parent"
                        app:icon="@drawable/ic_baseline_mark_email_read_24"
                        app:iconGravity="textStart"
                        android:enabled="false"
                        android:visibility="visible"
                        android:layout_marginTop="23dp"
                        android:layout_marginBottom="111dp"
                        android:text="@string/email_uc"
                        android:textSize="17sp"
                        android:paddingTop="23dp"
                        android:paddingBottom="23dp"
                        android:fontFamily="@font/montserratbold"
                        android:layout_width="320dp"
                        android:layout_height="wrap_content"/>

                    <View
                        android:id="@+id/practitioner_detail_footer_line"
                        app:layout_constraintTop_toBottomOf="@id/practitioner_detail_contact_email_button"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="33dp"
                        android:layout_marginBottom="111dp"
                        android:background="@color/chiron_grey"
                        android:elevation="13dp"
                        android:focusable="true"
                        android:focusableInTouchMode="true"
                        android:visibility="invisible" />






                </androidx.constraintlayout.widget.ConstraintLayout>



            </LinearLayout>







        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.core.widget.NestedScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>