<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".view.PivotDialogFragment"
    android:id="@+id/insight_dialog_view"
    android:layout_width="match_parent"
    android:layout_height="match_parent">




    <FrameLayout
        android:elevation="0dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">




        <com.google.android.material.card.MaterialCardView
            android:visibility="invisible"
            android:id="@+id/insight_dialog_info"
            app:cardCornerRadius="3dp"
            android:clickable="true"
            app:strokeColor="@color/chiron_deep_grey"
            app:strokeWidth="1dp"
            android:layout_marginTop="131dp"
            android:layout_marginStart="33dp"
            android:layout_marginEnd="69dp"
            app:cardElevation="69dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusable="true">

            <LinearLayout
                android:visibility="visible"
                android:orientation="vertical"
                android:background="@drawable/silver"
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <View
                    android:visibility="invisible"
                    android:elevation="3dp"
                    android:layout_marginTop="0dp"
                    android:layout_marginBottom="3dp"
                    android:background="@color/chiron_grey"
                    android:layout_width="match_parent"
                    android:focusableInTouchMode="true"
                    android:focusable="true"
                    android:layout_height="1dp"/>



                <iot.empiaurhouse.triage.utils.TypeWriterTextView
                    android:id="@+id/rendering_prompt"
                    android:visibility="invisible"
                    android:textSize="21sp"
                    android:text="@string/rendering_data"
                    android:lines="1"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:letterSpacing="0.10"
                    android:textStyle="normal"
                    android:layout_marginStart="6dp"
                    android:layout_marginTop="6dp"
                    android:layout_marginBottom="6dp"
                    android:textAllCaps="true"
                    android:textColor="@color/chiron_deep_grey"
                    android:fontFamily="@font/montserratmedium"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <View
                    android:elevation="3dp"
                    android:layout_marginTop="3dp"
                    android:layout_marginBottom="3dp"
                    android:background="@color/chiron_grey"
                    android:layout_width="match_parent"
                    android:focusableInTouchMode="true"
                    android:focusable="true"
                    android:layout_height="1dp"/>

                <TextView
                    android:visibility="invisible"
                    android:id="@+id/insight_label_title"
                    android:letterSpacing="0.13"
                    android:layout_marginTop="9dp"
                    android:layout_marginStart="6dp"
                    android:text="@string/vista_label"
                    android:fontFamily="@font/montserratlight"
                    android:maxLength="20"
                    android:lines="1"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:singleLine="true"
                    android:textSize="19sp"
                    android:textStyle="bold"
                    android:textColor="@color/chiron_blue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>


                <TextView
                    android:visibility="invisible"
                    android:id="@+id/insight_dm_title"
                    android:letterSpacing="0.13"
                    android:layout_marginTop="6dp"
                    android:layout_marginStart="6dp"
                    android:text="@string/data_model"
                    android:fontFamily="@font/montserratlight"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:textColor="@color/chiron_blue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:visibility="invisible"
                    android:id="@+id/insight_dm_title_text"
                    android:letterSpacing="0.33"
                    android:layout_marginTop="3dp"
                    android:layout_marginStart="6dp"
                    android:textSize="14sp"
                    android:text="@string/prescription"
                    android:fontFamily="@font/montserratlight"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>




                <TextView
                    android:visibility="invisible"
                    android:id="@+id/ep_text"
                    android:letterSpacing="0.33"
                    android:layout_marginTop="3dp"
                    android:layout_marginStart="6dp"
                    android:textSize="14sp"
                    android:text="@string/brand_name"
                    android:lines="1"
                    android:maxLines="1"
                    android:fontFamily="@font/montserratlight"
                    android:textStyle="normal"
                    android:textColor="@color/black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <View
                    android:elevation="3dp"
                    android:layout_marginTop="9dp"
                    android:layout_marginBottom="1dp"
                    android:background="@color/chiron_grey"
                    android:layout_width="match_parent"
                    android:focusableInTouchMode="true"
                    android:focusable="true"
                    android:layout_height="1dp"/>



                <TextView
                    android:visibility="invisible"
                    android:id="@+id/vt_title"
                    android:letterSpacing="0.13"
                    android:layout_marginTop="13dp"
                    android:layout_marginStart="44dp"
                    android:text="@string/data_vista"
                    android:fontFamily="@font/montserratlight"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:textAllCaps="true"
                    android:textColor="@color/chiron_blue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>


                <TextView
                    android:visibility="invisible"
                    android:id="@+id/vt_text"
                    android:letterSpacing="0.33"
                    android:layout_marginTop="3dp"
                    android:layout_marginBottom="3dp"
                    android:layout_marginStart="44dp"
                    android:textSize="14sp"
                    android:text="@string/histogram"
                    android:fontFamily="@font/montserratlight"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <View
                    android:elevation="3dp"
                    android:layout_marginTop="9dp"
                    android:layout_marginBottom="3dp"
                    android:background="@color/chiron_grey"
                    android:layout_width="match_parent"
                    android:focusableInTouchMode="true"
                    android:focusable="true"
                    android:layout_height="1dp"/>


                <TextView
                    android:visibility="gone"
                    android:id="@+id/range_insights_title"
                    android:letterSpacing="0.13"
                    android:layout_marginTop="11dp"
                    android:layout_marginStart="44dp"
                    android:text="@string/range"
                    android:textAllCaps="true"
                    android:fontFamily="@font/montserratlight"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:textColor="@color/chiron_blue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:visibility="gone"
                    android:id="@+id/insight_range_text"
                    android:letterSpacing="0.06"
                    android:layout_marginTop="3dp"
                    android:layout_marginStart="44dp"
                    android:layout_marginEnd="0dp"
                    android:text="@string/_13_november_2021_13_november_2021"
                    android:textAllCaps="false"
                    android:fontFamily="@font/montserratlight"
                    android:textSize="13sp"
                    android:textStyle="normal"
                    android:textColor="@color/black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>


                <TextView
                    android:visibility="gone"
                    android:id="@+id/pi_insight_threshold_title"
                    android:letterSpacing="0.13"
                    android:layout_marginTop="3dp"
                    android:layout_marginStart="44dp"
                    android:text="@string/pi_symbol"
                    android:textAllCaps="false"
                    android:fontFamily="@font/montserratlight"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:textColor="@color/chiron_blue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:visibility="gone"
                    android:id="@+id/pi_insight_param_text"
                    android:letterSpacing="0.33"
                    android:layout_marginTop="6dp"
                    android:layout_marginStart="44dp"
                    android:textSize="14sp"
                    android:text="@string/pi_threshold_value"
                    android:fontFamily="@font/montserratlight"
                    android:maxLength="24"
                    android:lines="1"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:visibility="gone"
                    android:id="@+id/omega_insight_param_title"
                    android:letterSpacing="0.13"
                    android:layout_marginTop="3dp"
                    android:layout_marginStart="44dp"
                    android:text="@string/omega_symbol"
                    android:textAllCaps="true"
                    android:fontFamily="@font/montserratlight"
                    android:textSize="16sp"
                    android:textStyle="bold"
                    android:textColor="@color/chiron_blue"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>

                <TextView
                    android:visibility="gone"
                    android:id="@+id/omega_insight_param_text"
                    android:letterSpacing="0.33"
                    android:layout_marginTop="6dp"
                    android:layout_marginBottom="6dp"
                    android:layout_marginStart="44dp"
                    android:textSize="14sp"
                    android:text="@string/omega_threshold_value"
                    android:maxLength="24"
                    android:lines="1"
                    android:maxLines="1"
                    android:ellipsize="end"
                    android:fontFamily="@font/montserratlight"
                    android:textStyle="bold"
                    android:textColor="@color/black"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>



                <View
                    android:elevation="3dp"
                    android:layout_marginTop="9dp"
                    android:layout_marginBottom="3dp"
                    android:background="@color/chiron_grey"
                    android:layout_width="match_parent"
                    android:focusableInTouchMode="true"
                    android:focusable="true"
                    android:layout_height="1dp"/>


                <ProgressBar
                    android:id="@+id/insight_progress"
                    android:visibility="invisible"
                    android:elevation="69dp"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:theme="@style/PivotProgress"
                    android:layout_gravity="center"
                    android:layout_marginTop="11dp"
                    android:layout_marginBottom="13dp"
                    android:layout_width="40dp"
                    android:layout_height="40dp"/>




            </LinearLayout>

        </com.google.android.material.card.MaterialCardView>

        <ImageView
            android:id="@+id/insight_triage_bot"
            android:layout_marginTop="-39dp"
            android:layout_marginBottom="69dp"
            android:visibility="invisible"
            android:elevation="69dp"
            android:src="@drawable/triage_insight_bot"
            android:layout_width="255dp"
            android:layout_height="255dp"
            android:contentDescription="@string/pivot_loader" />


    </FrameLayout>


</LinearLayout>